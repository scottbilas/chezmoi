{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "togglePaneZoom",
            "keys": "ctrl+alt+z"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "ctrl+alt+h"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "ctrl+alt+l"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "ctrl+alt+minus"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "ctrl+alt+j"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "ctrl+alt+k"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "ctrl+alt+shift+k"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "ctrl+alt+shift+j"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "ctrl+alt+shift+h"
        },
        {
            "command": 
            {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "ctrl+alt+shift+l"
        },
        {
            "command": "unbound",
            "keys": "f11"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "ctrl+alt+\\"
        }
    ],
    "alwaysShowTabs": false,
    "copyFormatting": "all",
    "copyOnSelect": true,
    "defaultProfile": "{58ad8b0c-3ef8-5f4d-bc6f-13e4c00f2530}",
    "initialCols": 150,
    "initialPosition": "50,50",
    "initialRows": 50,
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "newTabPosition": "afterCurrentTab",
    "profiles": 
    {
        "defaults": 
        {
            "antialiasingMode": "cleartype",
            "bellStyle": 
            [
                "window",
                "taskbar"
            ],
            "colorScheme": "One Half Dark",
            "font": 
            {
                "face": "FiraCode Nerd Font",
                "size": 8.0,
                "weight": "medium"
            },
            "historySize": 100000,
            "padding": "3",
            "tabColor": "#005B8F"
        },
        "list": 
        [
            {
                "colorScheme": 
                {
                    "dark": "Sonokai Shusia",
                    "light": "Campbell Powershell"
                },
                "commandline": "wsl -e bash --login -c tmux",
                "guid": "{58ad8b0c-3ef8-5f4d-bc6f-13e4c00f2530}",
                "hidden": false,
                "icon": "{{.profile.chezmoi_dirf}}/resources/icons/cli-2.png",
                "name": "tmux",
                "source": "Windows.Terminal.Wsl",
                "tabTitle": "Tmux"
            },
            {
                "commandline": "pwsh /nologo",
                "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
                "hidden": false,
                "name": "pwsh",
                "source": "Windows.Terminal.PowershellCore",
                "tabTitle": "Posh"
            },
            {
                "commandline": "wsl -e /home/linuxbrew/.linuxbrew/bin/zsh --login",
                "guid": "{482f5753-14e3-4dcc-a557-48f7dad1eda7}",
                "icon": "{{.profile.chezmoi_dirf}}/resources/icons/debian_logo_red.png",
                "name": "zsh",
                "startingDirectory": "~",
                "tabTitle": "zsh"
            }
{{- includeTemplate "../../private/WindowsTerminal/settings.json.tmpl" . }}
        ]
    },
