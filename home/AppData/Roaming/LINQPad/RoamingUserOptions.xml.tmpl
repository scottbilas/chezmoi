<?xml version="1.0" encoding="utf-8"?>
<UserOptions xmlns="http://schemas.datacontract.org/2004/07/LINQPad">
  <AlwaysShelveOnExit>true</AlwaysShelveOnExit>
  <BrowserEngine>WebView2</BrowserEngine>
  <ConvertTabsToSpaces>true</ConvertTabsToSpaces>
  <DefaultQueryLanguage>Statements</DefaultQueryLanguage>
  <FreshAppDomains>true</FreshAppDomains>
  <MaxQueryRows>1000</MaxQueryRows>
  <MemberOrderMode>0</MemberOrderMode>
  <NoNativeKeysQuestion>true</NoNativeKeysQuestion>
  <ShowLineNumbersInEditor>true</ShowLineNumbersInEditor>
  <UseDarkTheme>{{eq .profile.theme "dark"}}</UseDarkTheme>
</UserOptions>